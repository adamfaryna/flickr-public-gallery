{
  "name": "flickr-public-gallery",
  "version": "1.0.0",
  "description": "Simple Flickr public gallery with infinite scroll.",
  "author": "Adam Faryna <adamfaryna@appdy.net>",
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --progress -d --config webpack.dev.config.js",
    "prod": "webpack --progress -p --config webpack.prod.config.js",
    "test": "nyc mocha --require tests/setup.js --recursive src/tests/**/*.test.js",
    "test:debug": "mocha --inspect-brk --require tests/setup.js --recursive src/tests/**/*.test.js",
    "test:watch": "mocha --require tests/setup.js -w --recursive src/tests/**/*.test.js"
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "redux-saga",
    "webpack",
    "sass",
    "tdd",
    "bdd"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.6.0",
    "nyc": "^11.2.1",
    "react-hot-loader": "^3.1.1",
    "react-test-renderer": "^16.0.0",
    "responsive-loader": "^1.1.0",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "sharp": "^0.18.4",
    "sinon": "^4.0.2",
    "style-loader": "^0.19.0",
    "swig-webpack-plugin": "^0.5.1",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "jsonp": "^0.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "rxjs": "^5.5.2",
    "sanitize-html": "^1.15.0"
  }
}
