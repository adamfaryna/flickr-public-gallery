{
  "name": "flickr-public-gallery",
  "version": "1.0.0",
  "description": "Simple Flickr public gallery with infinite scroll.",
  "author": "Adam Faryna <adamfaryna@appdy.net>",
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --progress --config webpack.dev.config.js",
    "prod": "webpack --progress --config webpack.prod.config.js",
    "test": "mocha --require tests/setup.js --recursive src/tests/**/*.test.js",
    "test:watch": "mocha --require tests/setup.js -w --recursive src/tests/**/*.test.js"
  },
  "keywords": [
    "es6",
    "react",
    "redux",
    "redux-saga",
    "webpack",
    "sass"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.0",
    "swig-webpack-plugin": "^0.5.1",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  }
}
